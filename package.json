  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gititGoro/Coupon/issues"
  },
  "homepage": "https://github.com/gititGoro/Coupon#readme",
  "dependencies": {
    "@typechain/ethers-v5": "^11.1.2",
    "typechain": "^8.3.2"
  }
}